{
	"editor.suggestSelection": "recentlyUsedByPrefix",
	"editor.suggest.showWords": false,
	"lua.targetVersion": "5.1",
	"Lua.runtime.version": "Lua 5.1",
	"Lua.diagnostics.globals": [
    "Spring",
    "Game",
    "CMD",
    "widget",
    "Script",
    "WG",
    "VFS",
    "UnitDefs",
    "CMD_JUMP",
    "CMD_LEVEL",
    "CMD_RAW_MOVE",
    "CMD_RAW_BUILD",
    "CMD_AREA_MEX",
    "CMD_AREA_TERRA_MEX",
    "GL",
    "GL_TEXTURE_FILTER_CONTROL",
    "GL_TEXTURE_LOD_BIAS",
    "GL_RGBA16F_ARB",
    "GL_RGBA32F_ARB",
    "GL_DEPTH_COMPONENT",
    "GL_DEPTH_COMPONENT16",
    "GL_DEPTH_COMPONENT24",
    "GL_DEPTH_COMPONENT32",
    "GL_RGB",
    "GL_RGBA",
    "GL_DEPTH_COMPONENT",
    "GL_STENCIL_INDEX",
    "GL_FRAMEBUFFER_EXT",
    "GL_READ_FRAMEBUFFER_EXT",
    "GL_DRAW_FRAMEBUFFER_EXT",
    "GL_FRAMEBUFFER_COMPLETE_EXT",
    "GL_FRAMEBUFFER_INCOMPLETE_ATTACHMENT_EXT",
    "GL_FRAMEBUFFER_INCOMPLETE_MISSING_ATTACHMENT_EXT",
    "GL_FRAMEBUFFER_INCOMPLETE_DUPLICATE_ATTACHMENT_EXT",
    "GL_FRAMEBUFFER_INCOMPLETE_DIMENSIONS_EXT",
    "GL_FRAMEBUFFER_INCOMPLETE_FORMATS_EXT",
    "GL_FRAMEBUFFER_INCOMPLETE_DRAW_BUFFER_EXT",
    "GL_FRAMEBUFFER_INCOMPLETE_READ_BUFFER_EXT",
    "GL_FRAMEBUFFER_UNSUPPORTED_EXT",
    "GL_FRAMEBUFFER_STATUS_ERROR_EXT",
    "GL_COLOR_ATTACHMENT0_EXT",
    "GL_COLOR_ATTACHMENT1_EXT",
    "GL_COLOR_ATTACHMENT3_EXT",
    "GL_COLOR_ATTACHMENT4_EXT",
    "GL_COLOR_ATTACHMENT5_EXT",
    "GL_COLOR_ATTACHMENT6_EXT",
    "GL_COLOR_ATTACHMENT7_EXT",
    "GL_COLOR_ATTACHMENT8_EXT",
    "GL_COLOR_ATTACHMENT9_EXT",
    "GL_COLOR_ATTACHMENT10_EXT",
    "GL_COLOR_ATTACHMENT11_EXT",
    "GL_COLOR_ATTACHMENT12_EXT",
    "GL_COLOR_ATTACHMENT13_EXT",
    "GL_COLOR_ATTACHMENT14_EXT",
    "GL_COLOR_ATTACHMENT15_EXT",
    "GL_DEPTH_ATTACHMENT_EXT",
    "GL_STENCIL_ATTACHMENT_EXT",
    "gl",
    "widgetHandler",
    "LOG",
    "LUAUI_DIRNAME",
    "include",
    "FeatureDefs",
    "FeatureDefNames",
    "fontHandler",
    "YellowStr",
    "socket",
    "GL_HINT_BIT",
	],
	"Lua.workspace.ignoreDir": [
			".vscode",
			".github",
			".script",
	],
	"Lua.runtime.builtin": {
		"basic": "enable",
		"builtin": "enable",
		"table": "enable",
		"math": "enable",
		"string": "enable",
		"debug": "enable",
		"os": "enable",
		"coroutine": "enable",
		"io": "enable",
		"package": "disable",
		"bit32": "disable",
		"jit": "disable",
		"bit": "disable",
		"ffi": "disable",
		"utf8": "disable"
	},
	"Lua.workspace.library": [
	],
	"Lua.intelliSense.searchDepth": 3,
	"Lua.diagnostics.workspaceDelay": 50,
	"git.autofetch": "all",
	"markdownlint.config": {
		"MD033": false,
		"MD034": false,
	},
	"restructuredtext.confPath": "${workspaceFolder}\\predocs",
	"restructuredtext.builtDocumentationPath" : "${workspaceFolder}/docs/_build/html",
	"restructuredtext.updateOnTextChanged"    : "false",
	"restructuredtext.updateDelay": 1000,
	"Lua.diagnostics.disable": [
    "lowercase-global",
    "undefined-doc-name",
    "undefined-doc-class",
    "duplicate-set-field"
  ],
	"triggerTaskOnSave.tasks": {
		"Luacheck": ["*.lua"]
	},
	"Lua.workspace.userThirdParty": [
	],
}